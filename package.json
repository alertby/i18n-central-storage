    "@babel/preset-env": "^7.18.10",
    "@babel/register": "^7.18.9",
    "debug": "^2.6.9",
    "eslint": "^8.21.0",
    "mocha": "^10.0.0",
    "should": "*"
  },
  "dependencies": {
